{
  "devDependencies": {
    "@redocly/cli": "^2.11.1",
    "@stoplight/prism-cli": "^5.14.2",
    "@stoplight/spectral": "^6.5.0",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "openapi-typescript": "^7.10.1",
    "prisma": "6.19.0"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cli": "^11.0.12",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/testing": "^11.1.9",
    "@prisma/client": "6.19.0",
    "@zilliz/milvus2-sdk-node": "^2.6.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express": "^5.1.0",
    "swagger-ui-express": "^5.0.1"
  },
  "scripts": {
    "docs:lint": "redocly lint docs/openapi/openapi.yaml || spectral lint docs/openapi/openapi.yaml",
    "docs:mock": "prism mock docs/openapi/openapi.yaml -p 4010",
    "types:gen": "openapi-typescript docs/openapi/openapi.yaml -o apps/be/src/types/api.d.ts",
    "docs:bundle": "redocly bundle docs/openapi/openapi.yaml -o docs/openapi/bundle.yaml",
    "build": "nest build be",
    "start": "node dist/apps/be/main.js",
    "start:dev": "nest start be --watch",
    "start:prod": "node dist/apps/be/main.js",
    "prisma:generate": "prisma generate --schema=apps/be/prisma/schema.prisma",
    "postinstall": "prisma generate --schema=apps/be/prisma/schema.prisma"
  }
}
